# .env file

# Path to your Google Cloud Service Account Key file
# IMPORTANT: Keep this file secure and DO NOT commit it to version control!
GOOGLE_APPLICATION_CREDENTIALS="./keys/secrets.json"

# Your Google Cloud Project ID
GOOGLE_CLOUD_PROJECT="documentintelligence-460207"

# The location of your Document AI processor (e.g., us, eu)
DOCUMENTAI_LOCATION="us"

# The ID of your Document AI processor (e.g., b3b0327)
DOCUMENTAI_PROCESSOR_ID="83ff94085b0d2300"

# Google Generative AI (Gemini) Configuration
GOOGLE_GENERATIVE_AI_API_KEY="your-gemini-api-key" # Your Gemini API key
GEMINI_MODEL_NAME="gemini-pro" # Or gemini-1.5-pro, gemini-1.0-pro, etc.